	{% endblock %}

	<link rel="stylesheet" type="text/css" href="//cdnjs.cloudflare.com/ajax/libs/cookieconsent2/3.1.0/cookieconsent.min.css" />
	<script src="//cdnjs.cloudflare.com/ajax/libs/cookieconsent2/3.1.0/cookieconsent.min.js"></script>
	<script>
	var cgu ="<a href='http://127.0.0.1:8000/CGU'><u>CGU</u></a>";
	var privacy = "<a href='http://127.0.0.1:8000/privacy'><u>politique de confidentialité</u></a>";
		window.addEventListener("load", function(){
		window.cookieconsent.initialise({
			"palette": {
				"popup": {
					"background": "#ff004c",
					"text": "#f7f7f7"
				},
				"button": {
					"background": "#ffae00",	
					"text": "#f7f7f7"		
				}
			},
			"showLink": false,
			"theme": "classic",
			"content": {
				"message": "En poursuivant votre navigation sur notre site, vous acceptez nos ".concat(cgu)+" et notre ".concat(privacy)+".",
				"dismiss": "J'ai compris!",
				"link": "En savoir plus"
			}
		})});
	</script>
</head>

<body>
	{% block preloader %}
		<div class="preloader">
			<img src="img/loader.gif" alt="Preloader image">
		</div>
	{% endblock %}

	{% block navbar %}
		<nav class="navbar">
			<div class="container">
				<!-- Brand and toggle get grouped for better mobile display -->
				<div class="navbar-header">
					{% block mobileNav %}
						<button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
							<span class="sr-only">Toggle navigation</span>
							<span class="icon-bar"></span>
							<span class="icon-bar"></span>
							<span class="icon-bar"></span>
						</button>
					{% endblock %}
					{% block logo %}<a class="navbar-brand" href="#pageStart"><img src="img/CC_mini_logo.png" data-active-url="img/CC_mini_logo.png" alt=""></a>
				</div>{% endblock %}
				<!-- Collect the nav links, forms, and other content for toggling -->
				<div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
					<ul class="nav navbar-nav navbar-right main-nav">
						{% block navbarButtons %}

						{% endblock %}
					</ul>
				</div>
					{% block flashMessages %}{% endblock %}
				<!-- /.navbar-collapse -->
			</div>
			<!-- /.container-fluid -->
		</nav>
	{% endblock %}

	{% block header %}
		<header class="main-header" id="intro">
			<div class="container" id="pageStart">
				<div class="table">
					<div class="header-text">
						<div class="row">
							<div class="col-md-12 text-center">
								<h1 class="light white">Corse Connexion</h1>
								<h2 class="orange typed">{% block pageTitle %}{% endblock %}</h2>
							</div>
						</div>
					</div>
				</div>
			</div>
		</header>
	{% endblock %}

	{% block body %}

		{% block intro %}{% endblock %}	

		{% block services %}{% endblock %}

		{% block offers %}{% endblock %}

		{% block presentation %}
			<section id="pricing" class="section">
				<div class="container" id="presentation">
					<div class="row title text-center">
					</div>
					<div class="row no-margin" style="text-align: -webkit-right;">
						<div class="whoIs" >
							<div class="marjo">
							</div>
							<div class="team-info">
								<h4 class="white heading">Marjorie Di Placido Pietri</h4>
								<p class="description">#38ans #DigitalNomad #Punchy<br/>Marjorie n’hésite pas à bousculer les codes quand il s’agit de recrutement. Son anti-conformisme, son expertise et sa connaissance de l’écosystème digital sur l’île seront des atouts incontestables pour réussir ton projet.</p>
							</div>
						</div>
						<div class="whoIs" >
							<div class="greg">
							</div>
							<div class="team-info">
								<h4 class="white heading">Grégory Guiné</h4>
								<p class="description">#35ans #HardWorker #Passionate <br>Amoureux de la Corse, Grégory est passionné par le numérique et aime sortir de sa zone de confort. Son implication, ses compétences et sa bonne humeur en font un partenaire de choix pour la réussite de tes projets.</p>
							</div>
						</div>
						{# Masqué au dessus de width 992px #}
						<div class="whoIs-mobile" >
							<div class="row title text-center">
								<h2>Qui sommes nous?</h2>
							</div>
							<div class="team-info">
								<h4 class="white heading">Marjorie Di Placido Pietri</h4>
								<p class="description">#38ans #DigitalNomad #Punchy <br>Marjorie n’hésite pas à bousculer les codes quand il s’agit de recrutement. Son anti-conformisme, son expertise et sa connaissance de l’écosystème digital sur l’île seront des atouts incontestables pour réussir ton projet.</p>
							</div>
							<div class="team-info">
								<h4 class="white heading">Grégory Guiné</h4>
								<p class="description">#35ans #HardWorker #Passionate <br>Amoureux de la Corse, Grégory est passionné par le numérique et aime sortir de sa zone de confort. Son implication, ses compétences et sa bonne humeur en font un partenaire de choix pour la réussite de tes projets.</p>
							</div>
						</div>
					</div>
				</div>
			</section>
		{% endblock %}

		{% block goldenBook %}{% endblock %}

		{% block footer %}

		<footer>
			<div id="contact" class="container">
				<div class="row">
					<div class="col-sm-6 text-center-mobile">
						<form method="post" action="/getMessage">
							<div class="form-check form-check-inline">
								<label class="form-check-label" for="contactType1">Tu es un talent</label>
								<input class="form-check-input radioContact" type="radio" name="contactType" id="contactType1" value="talent" required >
								<label class="form-check-label" for="contactType2">Tu es une entreprise</label>
								<input class="form-check-input radioContact" type="radio" name="contactType" id="contactType2" value="entreprise" required >
							</div>
							<div class="form-group">
								<input type="text" class="form-control" name="name" placeholder="Nom" required >
							</div>
							<div class="form-group">
								<input type="email" class="form-control" name="email" placeholder="Mail" required >
							</div>
							<div class="form-group">
								<input type="tel" name="phone" class="form-control" placeholder="Téléphone" required>
							</div>
							<div class="form-group">
								<textarea class="form-control" name="message" rows="3" placeholder="Message" required ></textarea>
							</div>
							<p>Ces données ne sont en aucun cas susceptibles d'être revendues. Pas d'inquiètude, on est pas comme ça.</p>
							<button type="submit" class="btn btn-blue">Envoyer</button>
						</form>
					</div>
				</div>
				<div class="row bottom-footer text-center-mobile">
					<div class="col-sm-8 legal">
						<li><p>&copy; 2019 Tous droits réservés</p></li>
						<li><a href="/privacy"><p>Politique de confidentialité</p></a></li>
						<li><a href=""><p>Mentions légales</p></a></li>
					</div>
					{# <div class="col-sm-4 text-right text-center-mobile">
						<ul class="social-footer">
							<li><a href="https://www.facebook.com/"><i class="fa fa-facebook"></i></a></li>
							<li><a href="https://twitter.com/Kalapyu"><i class="fa fa-twitter"></i></a></li>
							<li><a href="https://www.linkedin.com/in/greg-guine/"><i class="fa fa-linkedin"></i></a></li>
						</ul>
					</div> #}
				</div>
			</div>
		</footer>

	{% endblock %}

	{% endblock %}

	{% block securityForms %}
		<div class="modal fade" id="modalLogin" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
			<div class="modal-dialog">
				<div class="modal-content modal-popup">
					<a href="#" class="close-link" data-dismiss="modal"><i class="icon_close_alt2"></i></a>
					<div class="form-control form-white">
						<h3 class="white">Connexion</h3>
						<form action="" class="popup-form">
							<input type="text" name="email" class="form-control form-white" placeholder="Email" required>
							<input type="password" name="password" class="form-control form-white" placeholder="Mot de Passe" required>
							{% block loginRedirectionPath %} {% endblock %} 
							<button type="submit" class="btn btn-submit" formaction="login" formmethod="post">Envoyer</button>
							<a href="/lostPassword">J'ai oublié mon mot de passe !!</a>
						</form>
					</div>
				</div>
			</div>
		</div>

		<div class="modal fade" id="modalRegistrationTalent" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
			<div class="modal-dialog">
				<div class="modal-content modal-popup">
					<a href="#" class="close-link" data-dismiss="modal"><i class="icon_close_alt2"></i></a>
					<div class="form-control form-white">
						<h3 class="white">Enregistrement Talent</h3>
						<p>Si tu es une entreprise. <a href="/pro">C'est par là!!!</a></p>
						<form action="insertdata.php" onsubmit="return checkall();" class="popup-form" enctype="multipart/form-data">
							<input type="text" name="lastname" class="form-control form-white" placeholder="Nom" required>
							<input type="text" name="firstname" class="form-control form-white" placeholder="Prénom" required>
							<input type="text" name="title" class="form-control form-white" placeholder="Métier" required>
							<input type="text" id="talentEmail" name="email" class="form-control form-white" placeholder="Email" required>
							<span id="talent_email_status"></span>
							<input type="password" name="password" class="form-control form-white" placeholder="Mot de passe" id="password_talent" required>
							<input type="password" name="password" class="form-control form-white" placeholder="Confirmez le mot de passe" id="confirm_password_talent" required>

							{# <div class="dropdown">
								<button id="dLabel" name="skills" class="form-control form-white dropdown" type="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
									Compétences
								</button>
								<div class="dropdown-menu animated fadeIn" role="menu" aria-labelledby="dLabel">
									<div class="animated lightSpeedIn">
										{% for skill in skills %}
											<label style="color: #ffae00" for="{{ skill.name }}">
												<input type="checkbox" id="{{ skill.name }}" name="{{ skill.name }}" value="{{ skill.name }}">{{ skill.name }}
											</label>
										{% endfor %}
									</div>
								</div>
							</div> #}

							<input type="text" name="location" class="form-control form-white" placeholder="Commune" required>
							
							<div class="form-control form-white file-input">
								<label for="avatar">Importez une photo ou laissez Corse-Connexion choisir un avatar pour vous:</label>
								<input type="file" name="avatar" accept="image/png, image/jpeg, image/jpg">
							</div>

							<p class="form-white">Pour que votre profil soit validé par un admin, merci d'ajouter votre profil LinkedIn ou votre CV au format PDF.</P>
							<div class="form-control form-white file-input">
								<label for="CV">Importez votre CV:</label>
								<input type="file" name="CV" accept="application/pdf">
							</div>
							<input type="text" name="linkedIn" class="form-control form-white" placeholder="Profil LinkedIn">
							
							<div class="form-control form-white file-input">
								<label>Informations complémentairtes</label>
								<div class="form-white">
									<label for="mobility">Mobile en Corse</label>
									<input type="checkbox" id="mobility" name="mobility">
								</div>

								<div class="form-white">
									<label for="isFreelance">Dispo pour des missions en Free</label>
									<input type="checkbox" id="isFreelance" name="isFreelance">
								</div>

								<div class="form-white">
									<label for="remoteOnly">Télétravail seulement</label>
									<input type="checkbox" class="form-white" id="remoteOnly" name="remoteOnly">
								</div>
							</div>
							<button type="submit" class="btn btn-submit" formaction="talentRegistration" formmethod="post">Envoyer</button>
						</form>
					</div>
				</div>
			</div>
		</div>

		<div class="modal fade" id="modalRegistrationCompany" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
			<div class="modal-dialog">
				<div class="modal-content modal-popup">
					<a href="#" class="close-link"><i class="icon_close_alt2"></i></a>
					<div class="form-control form-white">
						<h3 class="white">Enregistrement Entreprise</h3>
						<p>Si tu es un Talent. <a href="/talent">C'est par là!!!</a></p>
						<form action="" class="popup-form" enctype="multipart/form-data">
							<input type="text" name="name" class="form-control form-white" placeholder="Nom de l'entreprise" required>
							<input type="text" id="companyEmail" name="email" class="form-control form-white" placeholder="Email" required>
							<span id="company_email_status" style="color: #ff004c"></span>
							<input type="password" name="password" class="form-control form-white" placeholder="Mot de passe" id="password_company" required>
							<input type="password" name="password" class="form-control form-white" placeholder="Confirmez le mot de passe" id="confirm_password_company" required>
							<input type="text" name="address" class="form-control form-white" placeholder="Adresse" required>
							<input type="text" name="phone" class="form-control form-white" placeholder="Téléphone" required>
							<div class="form-control form-white file-input">
								<label for="picture">Importez votre logo</label>
								<input type="file" name="picture" accept="image/png, image/jpeg, image/jpg" required>
							</div>
							<button type="submit" id="company-test-mail" class="btn btn-submit" formaction="companyRegistration" formmethod="post">Envoyer</button>
						</form>
					</div>
				</div>
			</div>
		</div>

		<div class="modal fade" id="modalGetEmail" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
			<div class="modal-dialog">
				<div class="modal-content modal-popup">
					<a href="#" class="close-link" data-dismiss="modal"><i class="icon_close_alt2"></i></a>
					<div class="form-control form-white">
						<h4 class="white">Inscris toi pour recevoir les infos de la communauté et tes invitations aux évènements de Corse-Connexion!</h4>
						<form action="" class="popup-form">
							<input type="text" name="email" class="form-control form-white" placeholder="Email" required>
							<input type="text" name="firstname" class="form-control form-white" placeholder="Prénom" required>
							<input type="text" name="lastname" class="form-control form-white" placeholder="Nom" required>
							<input type="tel" name="phone" class="form-control form-white" placeholder="Téléphone" required>
							<p>Ces données ne sont en aucun cas susceptibles d'être revendues. Pas d'inquiètude, on est pas comme ça.</p>
							
							<button type="submit" class="btn btn-submit" formaction="getMail" formmethod="post">Envoyer</button>
						</form>
					</div>
				</div>
			</div>
		</div>
	{% endblock %}

	{% block form %}
	{% endblock %}
	
	<!-- Holder for mobile navigation -->
	<div class="mobile-nav">
		<ul>
		</ul>
		<a href="#" class="close-link"><i class="arrow_up"></i></a>
	</div>
	
	{% block scripts %}
		<!-- Scripts -->
		<script src="js/jquery-3.3.1.min.js"></script>
		<script src="js/jquery-1.11.1.min.js"></script>
		<script src="js/owl.carousel.min.js"></script>
		<script src="js/bootstrap.min.js"></script>
		<script src="js/wow.min.js"></script>
		<script src="js/typewriter.js"></script>
		<script src="js/jquery.onepagenav.js"></script>
		<script src="js/main.js"></script>
		<script>
			$(document).ready(function(){
				$('#talentEmail').change(function(){
					var talentEmail = $('#talentEmail').val();
					if (talentEmail != '')
					{
						$.ajax({
							url:"checkMail",
							method:"POST",
							data:{email:talentEmail},
							success:function(data){
								$('#talent_email_status').html(data);
							}
						})
					}
				})
				$('#companyEmail').change(function(){
					var companyEmail = $('#companyEmail').val();
					var companyButton = document.getElementById('company-test-mail');
					if (companyEmail != '')
					{
						$.ajax({
							url:"checkMail",
							method:"POST",
							data:{email:companyEmail},
							success:function(data){
								$('#company_email_status').html(data);
								if (data == ("Email disponible"))
								{
									companyButton.style.visibility = 'visible';
								}
								else 
								{
									companyButton.style.visibility = "hidden";
								}
							}
						})
					}
				})
			})
		</script>
	{% endblock %}
</body>

</html>
